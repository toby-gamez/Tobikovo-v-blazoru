@page "/ml-roz-vet-cleny"

<PageTitle>Rozvíjející větné členy</PageTitle>1

<section>
    <h1 class="verybig">Rozvíjející větné členy</h1>
    <ul class="grid">
        <li class="grid-item">
            <h3>Předmět</h3>
            <div class="grid-content">
                <button type="button" class="ripple-button" onclick="openPredmet()">Otevřít</button>
            </div>
        </li>
        <li class="grid-item">
            <h3>Přívlastek</h3>
            <div class="grid-content">
                <button type="button" class="ripple-button" onclick="openPrivlastek()">Otevřít</button>
            </div>
        </li>
        <li class="grid-item">
            <h3>Příslovečná určení</h3>
            <div class="grid-content">
                <button type="button" class="ripple-button2" onclick="openPrislovecne()">Otevřít</button>
            </div>
        </li>
        <li class="grid-item">
            <h3>Podmět</h3>
            <div class="grid-content">
                <button type="button" class="ripple-button" onclick="openPodmet()">Otevřít</button>
            </div>
        </li>
        <li class="grid-item">
            <h3>Přísudek</h3>
            <div class="grid-content">
                <button type="button" class="ripple-button" onclick="openPrisudek()">Otevřít</button>
            </div>
        </li>
        <li class="grid-item">
            <h3>Přístavek</h3>
            <div class="grid-content">
                <button type="button" class="ripple-button" onclick="openPristavek()">Otevřít</button>
            </div>
        </li>
    </ul>
</section>

<script>
    function openPredmet() {
        window.location.href = "ml-predmet"
    }
</script>
<script>
    function openPrivlastek() {
        window.location.href = "ml-privlastek"
    }
</script>
<script>
    function openPrislovecne() {
        window.location.href = "ml-prislovecne-urceni"
    }
</script>
<script>
    function openPodmet() {
        window.location.href = "ml-podmet";
    }
</script>
<script>
    function openPrisudek() {
        window.location.href = "ml-prisudek";
    }
</script>
<script>
    function openPristavek() {
        window.location.href = "ml-pristavek";
    }
</script>